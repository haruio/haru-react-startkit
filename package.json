{
  "name": "haru-react-startkit",
  "version": "1.0.0",
  "description": "react starter project",
  "main": "/app/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --config config/webpack/webpack.production.js --bail -p",
    "dev": "webpack-dev-server --history-api-fallback --hot --inline --config config/webpack/webpack.develop.js",
    "lint:js": "eslint --ignore-path .gitignore -- .",
    "lint:css": "stylelint ./app/**/*.css",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test karma start config/testing/karma.conf.js",
    "test:live": "cross-env NODE_ENV=test BABEL_ENV=test karma start config/testing/karma.conf.js --auto-watch --no-single-run",
    "test:clean": "rimraf coverage",
    "analyze": "cross-env NODE_ENV=production BABEL_ENV=production webpack --config webpack.production.js --profile --json > stats.json",
    "analyze:clean": "rimraf stats.json",
    "clean": "rimraf public && npm run test:clean && npm run analyze:clean",
    "start": "npm run dev"
  },
  "author": "pheadra",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "classnames": "^2.2.5",
    "debug": "^2.2.0",
    "fbjs": "^0.8.5",
    "immutable": "^3.8.1",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-router-scroll": "^0.3.2",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "clean-webpack-plugin": "^0.1.13",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.8.0",
    "eslint-plugin-react": "^6.4.1",
    "expect": "^1.20.2",
    "expect-jsx": "^2.6.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "0.6.5",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
